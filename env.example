# Dify Console API (self-hosted or cloud) base URL
# examples:
# - http://localhost
# - https://your-dify.example.com
# - https://cloud.dify.ai  (if your console is hosted there)
DIFY_BASE_URL=

# Console login (NOT App API key)
DIFY_EMAIL=
# Plaintext password here (script will Base64-encode as required by Dify)
DIFY_PASSWORD=

# SSL verification (true/false). If you use self-signed certs, set false.
DIFY_VERIFY_SSL=true


